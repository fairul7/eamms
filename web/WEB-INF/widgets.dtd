<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT config (page)*>

<!ELEMENT page (panel|form|portlet|tabbedpanel|label|link)*>
<!ATTLIST page
    name CDATA #REQUIRED
    stateless CDATA #IMPLIED
    url CDATA #REQUIRED
>

<!ELEMENT panel (panel|form|portlet|tabbedpanel|label|link)*>
<!ATTLIST panel
    name CDATA #REQUIRED
    stateless CDATA #IMPLIED
    text CDATA #IMPLIED
    columns CDATA #IMPLIED
    width CDATA #IMPLIED
>

<!ELEMENT form (label|link|textfield|password|textbox|richtextbox|selectbox|checkbox|radio|datefield|button)*>
<!ATTLIST form
    name CDATA #REQUIRED
    stateless CDATA #IMPLIED
    columns CDATA #IMPLIED
    width CDATA #IMPLIED
>

<!ELEMENT portlet (panel|form|portlet|tabbedpanel|label|link)*>
<!ATTLIST portlet
    name CDATA #REQUIRED
    stateless CDATA #IMPLIED
    text CDATA #IMPLIED
    columns CDATA #IMPLIED
    width CDATA #IMPLIED
